{
  "name": "laravel-docker",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d",
    "build": "docker-compose build",
    "stop": "docker-compose stop",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f",
    "shell:php": "docker-compose exec php sh",
    "shell:nginx": "docker-compose exec nginx sh",
    "shell:mysql": "docker-compose exec mysql sh",
    "shell:rabbitmq": "docker-compose exec rabbitmq sh",
    "shell:supervisor": "docker-compose exec supervisor sh",
    "composer": "docker-compose exec php composer",
    "artisan": "docker-compose exec php php artisan"
  }
}